{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "ios-swift": {
      "transformGroup": "ios-swift",
      "buildPath": "build/ios/",
      "files": [
        {
          "destination": "DesignTokens.swift",
          "format": "ios-swift/class.swift",
          "options": {
            "className": "DesignTokens"
          }
        }
      ]
    }
  }
}
